    <profile id="9045e242-71f8-6029-fc57-2b7cbedcc398" name="Archaeotech Pistol" publicationId="839b6f2a--pubN84244" page="82" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Weapon">
